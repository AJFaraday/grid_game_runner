<!DOCTYPE html>
<html>
  <head>
  </head>

  <body>
    <table>
      <thead>
        <tr>
          <th rowspan="2">Bot</th>
          <th rowspan="2">Score</th>
          <th colspan="2">Wins</th>
          <th rowspan="2">Draws</th>
          <th rowspan="2">Losses</th>
          <th rowspan="2">Actions</th>
        </tr>
        <tr>
          <th>Outright</th>
          <th>Timeout</th>
        </tr>
      </thead>
      <tbody>
        <% rankings.each do |ranking| %>
          <tr>
            <td><%= ranking['Bot'] %></td>
            <td><%= ranking['Score'] %></td>
            <td><%= ranking['Outright'] %></td>
            <td><%= ranking['Timeout'] %></td>
            <td><%= ranking['Draws'] %></td>
            <td><%= ranking['Losses'] %></td>
            <td><a href="bot_<%= ranking['Gist'] %>.html">Matches</a></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </body>

</html>
